function validarUsuario(usuario){

     		if(usuario == ""){
     			return false;
     		}

     		if(usuario.length<userNameMinimosCaracteresPermitidos){
     			return false;
     		}

     		if(debeTenerMasDeEstosNumeros(usuario,noPuedeTenerNumeros)==false){
     			return false;
     		}
     	}

     	function validarContrasenia(contrasenia){
     		if(contrasenia ==""){
     			return false;
     		}

     		if(contrasenia.length<passwordMinimosCaracteresPermitidos){
     			return false;
     		}

     		if(debeTenerAlmenosUnNumero(contrasenia,passwordMinimosNumerosRequeridos)==false){
     			return false;
     		}
     	} 